#include <stdio.h>
#include<iostream>
#include<stdlib.h>
using namespace std;
void update(int *a,int *b) {
   int aa=*a,bb=*b;

    *a= aa + bb;

    *b= aa- bb;
}

int main() {
     int a=10, b=15;
     int *pa = &a, *pb = &b;
   scanf("%d %d", &a, &b);
    update(pa, pb);
    printf("%d\n%d", a,abs( b));
    return 0;
}
