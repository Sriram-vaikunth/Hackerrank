//Variable length arrays


#include <vector>
#include <iostream>
using namespace std;


int main() {
    int n,s;
    cin>>n>>s;
    int **a=new int *[n];
    int temp;
    for(int i=0;i<n;i++)
    {
    cin>>temp;
    a[i]=new int[temp];
    for(int j=0;j<temp;j++)
    {
        cin>>a[i][j];
    }
    }
    int array_no,index;
    for(int i=0;i<s;i++)
    {
        cin>>array_no>>index;
        cout<<a[array_no][index]<<endl;   
     }


    return 0;
}
